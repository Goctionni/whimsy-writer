{
  "name": "hello-world",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run update-passages && vite build",
    "dev": "vite serve",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p ./tsconfig.app.json",
